<view>
    <view class="basic-info">
        <view class="job-name">{{jobId}}</view>
        <text class="calc-pair">样本数：<text class="data">{{sampleNum}}</text></text>
        <text class="calc-pair">取样时间：<text class="data">{{sampleTime}}</text></text>
        <text class="calc-pair">标准差：<text class="data">{{staDeviation}}</text></text>
    </view>
    <view class="can-wrapper">
        <!-- canvas-id和ec是必须传的 -->
        <ec-canvas canvas-id="job-trend" ec="{{trendOption}}"></ec-canvas>
    </view>
    <view class="can-wrapper">
        <!-- canvas-id和ec是必须传的 -->
        <ec-canvas canvas-id="job-salary" ec="{{salaryOption}}"></ec-canvas>
    </view>
</view>